{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "maas": {
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "url": "${DB_URL}",
    "database": "${DB_NAME}",
    "password": "${DB_PASS}",
    "name": "maas",
    "user": "${DB_USER}",
    "connector": "mongodb"
  },
  "gmail": {
    "name": "${SMTP_NAME}",
    "connector": "mail",
    "transports": [
      {
        "type": "SMTP",
        "host": "${SMTP_HOST}",
        "secure": true,
        "port": "${SMTP_PORT}",
        "auth": {
          "user": "${SMTP_USER}",
          "pass": "${SMTP_PASSWORD}"
        }
      }
    ]
  }
}
